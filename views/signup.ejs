<% include ./partial/upperLayout %>
    <div class='form-group'>
        <table>
        <form action='/signUp' method='POST' enctype="multipart/form-data">
            <tr>
                <td>User name </td>
                <td style="padding: 10px 10px 10px 10px"><input type='text' name='user_name' class='form-control'/></td>
            </tr>
            <tr>
                <td>Password </td>
                <td style="padding: 10px 10px 10px 10px"><input type='password' name='password' class='form-control'/></td>
            </tr>
            <tr>
                <td>Name </td>
                <td style="padding: 10px 10px 10px 10px"><input type='text' name='name' class='form-control'/></td>
            </tr>
            <tr>
                <td>Email id </td>
                <td style="padding: 10px 10px 10px 10px"><input type='text' name='email_id' class='form-control'/></td>
            </tr>
            <tr>
                <td>Profile photo </td>
                <td style="padding: 10px 10px 10px 10px"><input type='file' name='profile_image'/></td>
            </tr>
            <tr>
                <td><input type='submit' value='Sign up' class='btn btn-primary'/></td>
            </tr>
        </form>
    </table>
    </div>
<% include ./partial/lowerLayout %>